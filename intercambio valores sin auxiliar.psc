

Algoritmo  desafio_intercambio_variables_sin_auxiliar
	
	Definir num1, num2 Como Entero
	
	Escribir "primer numero";
	
	Leer num1;
	
	Escribir "segundo numero";
	
	Leer num2;
	
	num1<-num1+num2;
	
	num2<-num1-num2;
	
	num1<-num1-num2;
	
	
	Escribir "nuevo valor de num1 intercambiado "," ", num1
	
	Escribir "nuevo valor de num2 intercambiado "," ", num2
	
	
FinAlgoritmo

